{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [1, "single", "avoid-escape"],
    "curly": 0,
    "strict": [2, "global"],
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [1, "nofunc"],
    "no-new-func": 0,
    "prefer-const": 1,
    "no-var": 1
  },
  "extends": [
    "eslint:recommended"
  ]
}
